{
  "name": "calm-twitter",
  "private": true,
  "version": "1.0.3",
  "description": "This extension hides trending information from twitter timelines to make your mind calm.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard"
  },
  "standard": {
    "globals": [
      "chrome"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@types/chrome": "^0.x.x",
    "babel-cli": "6.x.x",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.x.x",
    "babel-loader": "8.x.x",
    "babel-preset-env": "1.x.x",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "0.x.x",
    "chromereload": "0.x.x",
    "del": "5.x.x",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "3.x.x",
    "gulp-cache": "1.x.x",
    "gulp-clean-css": "^4.x.x",
    "gulp-filter": "6.x.x",
    "gulp-git": "2.x.x",
    "gulp-if": "3.x.x",
    "gulp-imagemin": "7.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "4.x.x",
    "gulp-livereload": "4.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "^4.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "5.x.x",
    "require-dir": "1.x.x",
    "standard": "14.x.x",
    "ts-loader": "^6.x.x",
    "tslint": "^6.x.x",
    "typescript": "^3.x.x",
    "vinyl-named": "1.x.x",
    "webpack": "4.x.x",
    "webpack-stream": "5.x.x",
    "yargs": "15.x.x"
  },
  "dependencies": {
    "babel-register": "^6.26.0"
  }
}
